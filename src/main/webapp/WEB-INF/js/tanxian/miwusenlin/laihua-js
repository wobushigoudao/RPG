<span id="dabai" class="dabai"> <%
String dabai = (String) request.getAttribute("dabai");
 if (dabai != null) {
	 request.setAttribute("dabai", dabai);

 %> <script type="text/javascript">
 var a="${dabai}";
var r = document.getElementById("dabai");
var j=0;
 var xun = window.setInterval(function() {
 if(a.length==0){
	 clearInterval(xun);
 }else{
		r.innerHTML = r.innerHTML + a[j];
		j++;

		if (j >= a.length) {	
			clearInterval(xun)
 }
 }
 },50);
 
 </script>
  <%
 }
 %>
 
	</span>
	
